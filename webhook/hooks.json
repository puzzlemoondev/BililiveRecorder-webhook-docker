[
  {
    "id": "recorder-file-closed",
    "execute-command": "poetry",
    "pass-arguments-to-command": [
      {
        "source": "string",
        "name": "run"
      },
      {
        "source": "string",
        "name": "action"
      },
      {
        "source": "entire-payload"
      }
    ],
    "trigger-rule": {
      "match": {
        "type": "value",
        "value": "FileClosed",
        "parameter": {
          "source": "payload",
          "name": "EventType"
        }
      }
    }
  }
]
